  	<groups>
  		<run>
  			<include name = "smoke" />
  			<include name = "regression_1" />
  			<include name = "regression_2" />
  			
  		</run>
  	</groups>
    	<classes>
      		<class name="Test_Cases.Finding_Hospitals"/>
    	</classes>
  </test> 
  
   <test thread-count="5" name="Test_1">
  	<parameter name="browser" value="edge"></parameter>
    	<classes>
      		<class name="Test_Cases.Finding_Hospitals"/>
    	</classes>
  </test> 

</suite> <!-- Suite -->
